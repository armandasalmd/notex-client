{
  "hosting": {
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/testService",
        "run": {
          "serviceId": "notex-api-service",
          "region": "europe-west2"
        }
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
